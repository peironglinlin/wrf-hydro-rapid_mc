#
#  Options for Linux with pgf90
#
 COMPILERF90	=	pgf90
 FREESOURCE	=	-Mfree
 F90FLAGS	=       -g -byteswapio 
 MODFLAG	=	-I
 LDFLAGS	=	
 CPP		=	cpp
 CPPFLAGS	=	-C -P -traditional # -D_HRLDAS_URBAN_
 LIBS		=	
 LIBJASPER     =       -ljpeg -L/usr/lib -ljasper
 INCJASPER     =       -I/usr/include
# NETCDFMOD      =       -I/usr/local/netcdf-pgi/include
# NETCDFLIB      =       -L/usr/local/netcdf-pgi/lib -lnetcdf -lnetcdff
 NETCDFMOD      =       -I$(HOME)/netcdf/include
 NETCDFLIB      =       -L$(HOME)/netcdf/lib -lnetcdf -lnetcdff
 BZIP2          =       YES
 BZIP2_LIB      =       -lbz2
 BZIP2_INCLUDE  =       -I/usr/include
 RM             =       rm -f
 CC             =       cc

